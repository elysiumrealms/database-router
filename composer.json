{
    "name": "elysiumrealms/database-router",
    "type": "library",
    "description": "A Database Router isolate Read Write Connections",
    "keywords": ["laravel", "database", "master-slave", "router"],
    "authors": [
        {
            "name": "deflinhec",
            "email": "deflinhec@gmail.com"
        }
    ],
    "version": "1.0.1",
    "license": "MIT",
    "require": {
        "php": "*",
        "laravel/framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Elysiumrealms\\DatabaseRouter\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@prepare"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elysiumrealms\\DatabaseRouter\\DatabaseRouterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
